{
  "name": "npm-monorepo",
  "version": "1.0.0",
  "lockfileVersion": 3,
  "requires": true,
  "packages": {
    "": {
      "name": "npm-monorepo",
      "version": "1.0.0",
      "workspaces": [
        "packages/*"
      ],
      "devDependencies": {
        "typescript": "^5.0.0"
      }
    },
    "node_modules/@myorg/api": {
      "resolved": "packages/api",
      "link": true
    },
    "node_modules/@myorg/client": {
      "resolved": "packages/client",
      "link": true
    },
    "node_modules/@myorg/models": {
      "resolved": "packages/models",
      "link": true
    },
    "node_modules/express": {
      "version": "4.18.2",
      "resolved": "https://registry.npmjs.org/express/-/express-4.18.2.tgz",
      "dependencies": {
        "accepts": "~1.3.8",
        "body-parser": "1.20.1"
      }
    },
    "node_modules/accepts": {
      "version": "1.3.8",
      "resolved": "https://registry.npmjs.org/accepts/-/accepts-1.3.8.tgz"
    },
    "node_modules/body-parser": {
      "version": "1.20.1",
      "resolved": "https://registry.npmjs.org/body-parser/-/body-parser-1.20.1.tgz"
    },
    "node_modules/lodash": {
      "version": "4.17.21",
      "resolved": "https://registry.npmjs.org/lodash/-/lodash-4.17.21.tgz"
    },
    "node_modules/typescript": {
      "version": "5.3.3",
      "resolved": "https://registry.npmjs.org/typescript/-/typescript-5.3.3.tgz",
      "dev": true
    },
    "node_modules/zod": {
      "version": "3.22.4",
      "resolved": "https://registry.npmjs.org/zod/-/zod-3.22.4.tgz"
    },
    "packages/api": {
      "name": "@myorg/api",
      "version": "1.0.0",
      "dependencies": {
        "@myorg/models": "workspace:*",
        "express": "^4.18.2"
      }
    },
    "packages/client": {
      "name": "@myorg/client",
      "version": "1.0.0",
      "dependencies": {
        "@myorg/models": "workspace:*",
        "lodash": "^4.17.21"
      }
    },
    "packages/models": {
      "name": "@myorg/models",
      "version": "1.0.0",
      "dependencies": {
        "zod": "^3.22.4"
      }
    }
  }
}
